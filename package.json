		"build": "ts-node src/build.ts",
		"prettier:format": "yarn prettier --write 'src/**/*.{js,jsx,ts,tsx,css,json}'"
	},
	"dependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@figma/plugin-typings": "^1.55.1",
