		"@types/react": "^18.0.25",
		"@types/react-dom": "^18.0.8",
		"esbuild": "^0.15.13",
		"liquidjs": "^9.42.1",
		"prettier": "^2.7.1",
		"rimraf": "^3.0.2",
		"ts-node": "^10.9.1",
		"typescript": "^4.8.4"
	},
	"figma": {
		"name": "Figma Plugin Boilerplate",
		"id": "000000000000",
		"api": "1.0.0",
		"main": "main.js",
