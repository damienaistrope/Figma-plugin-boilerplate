{
	"name": "figma-plugin-boilerplate",
	"version": "1.0.0",
	"description": "Figma plugin boilerplate built with Typescript, React and ESbuild",
	"keywords": [
		"figma",
		"typescript",
		"esbuild",
		"react"
	],
	"author": "Zann St Pierre <mail@zann.com.au>",
	"license": "MIT",
	"repository": {
		"type": "private",
		"url": "https://github.com/zannstpierre/figma-plugin-boilerplate"
	},
	"scripts": {
